{
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier",
    "@typescript-eslint/eslint-plugin"
  ],
  "rules": {
    // "no-console": 2,
    // "no-debugger": "error",
    "prefer-const": "off",
    "no-undef": "off",
    "react/display-name": "off",
    "no-prototype-builtins": "off",
    "no-redeclare": "off"
  }
}